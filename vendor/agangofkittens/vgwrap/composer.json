{
  "name": "agangofkittens/vgwrap",
  "type": "library",
  "description": "A Laravel 5 wrapper for the Vainglory API",
  "keywords": [
    "agangofkittens",
    "vgwrap",
    "vainglory",
    "wrapper"
  ],
  "homepage": "https://github.com/agangofkittens/vgwrap",
  "license": "MIT",
  "authors": [
    {
      "name": "agangofkittens",
      "email": "agangofkittens@gmail.com",
      "homepage": "https://github.com/agangofkittens",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/agangofkittens/vgwrap/issues"
  },
  "require": {
    "php": "^7.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "agangofkittens\\vgwrap\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "agangofkittens\\vgwrap\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
